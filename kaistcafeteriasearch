# 각 키워드가 들어갈 위치에 {} 를 사용해주세요
location_q = 'Are you on the {} side of the school main gate?:'
day_q = 'IS today date is {}?:'
time_q = 'Is it {} right time for you:'
menu_q = 'Do you want to eat the {} menu'
time_list = ['breakfast', 'lunch', 'dinner']
script = """
카이스트 (1) 위치, (2) 날짜, (3) 시간, (4) 메뉴입력에 따른 식당 추론 질의 응답 프로그램입니다. 
각 단계별 질문을 보시고 질문에 따라 응답해주세요. 
"""

def check_search(knowledge_dict):
    '''
    답변을 직접 입력받아 선택 
    :param knowledge_dict: 술어논리 진행을 위해 구축된  지식
    :return: 선택한 결과 knowledge 반환
    '''
    print(", ".join([i for i in knowledge_dict.keys()])) # knowledge_dict 의 술어논리 진행을 위해 구축된  지식의 키값을 모두 , 로 분리해 가져옴
    while True:
        answer = input('Please enter a keyword :') 
        if answer not in knowledge_dict: # 없으면 다시 입력 메시지 보냄
            print('There is no data that corresponds to your input. Please type it again')
            continue
        return knowledge_dict[answer] # 있으면 답 반환

def select_enter(q):
    '''
    추론 형식을 knowledge base 제시후 yes/no 선택으로 할지 직접입력으로 할지 선택
    :return: 선택 결과 True, False
    '''
    answer = input('{} : '.format(q)) # 추론을 위한 input 형식을 knowledge base 제시후 yes/no 선택으로 할지 직접입력으로 할지 선택하기 위해 yes 또는 no를 인풋함
    if answer == 'y' or answer == 'Y' or answer == 'yes':
        return True  # True가 되면 knowledge base 제시후 직접 타이핑을 통해 input
    elif answer == 'n' or answer == 'N' or answer == 'no':
        return False # False가 되면 yes/no 선택을 통해 input
    print('There is no data that corresponds to your input. Please type it again.')
    return select_enter(q)





def check_stop(): # 처음부터 돌아가 계속 진행될지를 물어봄
    '''
    식당 메뉴 찾기를 이어서 할지 여부
    :return: True or False
    '''
    answer = input('Would you like to go on? : ')
    if answer == 'y' or answer == 'Y' or answer == 'yes':
        return False # 위에서 yes를 했을때 return False를 하게되면, 다시 처음으로 돌아감
    return True # False가 아니라 True가 되면 프로그램을 나감


def dict_check(knowledge_dict, q, select):
    '''
    dictionary 형식의 knowledge를 질의
    :param knowledge_dict:술어논리 진행을 위해 구축된  지식
    :param q:  질의 문장
    :param select: 답변 직접 선택 여부
    :return: 선택한 결과 knowledge 반환
    '''

    if select: #true값이면 check search함수를 통해 추론 응답 직접입력, false값이면 while true로 가서 제시된 knowledge base 중 원하는 것을 yes, no로 응답 입력 
        return check_search(knowledge_dict)
    while True: 
        for k in knowledge_dict.keys():
            answer = input(q.format(k))# yes/no 선택에서 해당 {} 키워드를 넣는 코드
            if answer == 'y' or answer == 'Y' or answer == 'yes':
                return knowledge_dict[k]
            print('You did not select the any choices. Please choose again.')
            

def time_check(knowledge, q, select):
    '''
    아침 점심 저녁을 체크
    :param knowledge:술어논리 진행을 위해 구축된  지식
    :param q: 질의 문장
    :param select: 답변 직접 선택 여부
    :return: 선택한 결과 knowledge 반환
    '''
    if select: #true값이면 아래 프린트후 추론 응답 직접입력, false값이면 아래 while true로 가서 제시된 knowledge base 중 원하는 것을 yes, no로 응답 입력 
        print('breakfast, lunch, dinner')
        while True:
            answer = input('Please enter a keyword :')
            result = None # 변수 선언
            if answer == 'breakfast':
                result = knowledge[0] #응답이 breakfast면, 결과는 knowledge 첫번째 list에서 찾음
            if answer == 'lunch':
                result = knowledge[1] #응답이 lunch면, 결과는 knowledge 두번째 list에서 찾음
            if answer == 'dinner':
                result = knowledge[2] #응답이 dinner면, 결과는 knowledge 세번째 list에서 찾음
            if answer is None:
                print('There is no data that corresponds to your input. Please type it again.')
                continue
            return result
    while True: #true가 아니면, 하나씩 해당하는 knowledge base를 물어봄
        for i, k in enumerate(['breakfast', 'lunch', 'dinner']):
            answer = input(q.format(k)) # yes/no 선택에서 해당 {} 키워드를 넣는 코드
            if answer == 'y' or answer == 'Y' or answer == 'yes':
                return knowledge[i]
        print('You did not select the any choices. Please choose again.')


def menu_check(knowledge, q, select):
    '''
    메뉴 체크
    :param knowledge:술어논리 진행을 위해 구축된  지식
    :param q: 질의 문장
    :param select: 답변 직접 선택 여부
    :return: 선택한 결과 knowledge 반환
    '''
    menu_index = {}
    for menus in knowledge: # 리스트 형식인 knowledge의 식당의 메뉴리스트를 하나씩 모두 불러옵니다
        restaurant = menus[1]
        for menu in menus[0]:# 리스트 형식인 knowledge의 메뉴를 하나씩 모두 불러옵니다
            if menu in menu_index: # key (menu)에 해당하는 답이 있으면 restaurant
                menu_index[menu].append(restaurant)
            else:
                menu_index[menu] = [restaurant] # key (menu)에 해당하는 답이 없으면 restaurant라는 list를 넣어서 만들어줌
    if select: #true값이면 아래 프린트후 추론 응답 직접입력, false값이면 아래 while true로 가서 제시된 knowledge base 중 원하는 것을 yes, no로 응답 입력 
        print(', '.join(menu_index.keys()))
        while True:
            answer = input('Please enter a keyword :')
            if answer in menu_index:
                return menu_index[answer]
            print('There is no data that corresponds to your input. Please type it again.')
    while True: #true가 아니면, 하나씩 물어보는것
        for k in menu_index.keys():
            answer = input(q.format(k))
            if answer == 'y' or answer == 'Y' or answer == 'yes':
                return menu_index[k]
        print('You did not select the any choices. Please choose again.')

def predicate_logic(knowledge_dict):
    '''
    술어논리 진행을 위해 구축된 사전 형식의 지식을 통해
    위치, 날짜, 시간(아침, 점심, 저녁), 메뉴를 확인해 식당을 반환
    :param knowledge_dict: 술어논리 진행을 위해 구축된 사전 형식의 지식 - knowledge base
    :return: None
    '''
    print(script)
    while True:
        # 위치 선택
        select = select_enter('Which side of the restaurants are you finding from the school main entrance? If you want to directly input text, type ‘yes’, and if you want to be provided with a location reference, type ‘no’.')
        knowledge = dict_check(knowledge_dict, location_q, select)  
        if knowledge is None:
            if check_stop():
                break
            continue
        # 날짜 선택
        select = select_enter('What date are you looking for? If you want to directly input text, type ‘yes’, and if you want to be provided with a date reference, type ‘no’.')
        knowledge = dict_check(knowledge, day_q, select)
        if knowledge is None:
            if check_stop():
                break
            continue
        # 시간 선택
        select = select_enter('When is the meal time you are looking for? If you want to directly input text, type ‘yes’, and if you want to be provided with a time reference, type ‘no’.')
        knowledge = time_check(knowledge, time_q, select)
        if knowledge is None:
            if check_stop():
                break
            continue
        # 메뉴 선택
        select = select_enter('What menu are you looking for? If you want to directly input text, type ‘yes’, and if you want to be provided with a menu reference, type ‘no’.')
        result = menu_check(knowledge, menu_q, select)
        if result is None: #만약 결과가 없을 경우
            if check_stop(): #check stop이 true이면, 프로그램을 종료하고 나가고, check stop이 아니면, 계속 진행
                break
            continue
            
        print('Go to restaurant {}.'.format(' or '.join(result))) # 추론을 통한 식당 결과를 받아 출력함
        answer = input('Would you like to continue? : ')
        if answer == 'y' or answer == 'Y' or answer == 'yes':
            continue
        break


predicate_logic(knowledge_dict)
